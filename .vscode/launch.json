{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Run all unit tests with pytest",
      "type": "python",
      "request": "launch",
      "console": "integratedTerminal",
      "envFile": ".env",
      "module": "pytest"
    },
    {
      "name": "Step-wise testing with pytest",
      "type": "python",
      "request": "launch",
      "console": "integratedTerminal",
      "envFile": ".env",
      "module": "pytest",
      "args": [
        "--sw"
      ]
    },
    {
      "name": "Launch a capellambse REPL",
      "type": "python",
      "request": "launch",
      "console": "integratedTerminal",
      "envFile": ".env",
      "pythonArgs": [
        "-Xdev"
      ],
      "module": "capellambse.repl"
    },
    {
      "name": "Interact with the 5.0 test model",
      "type": "python",
      "request": "launch",
      "console": "integratedTerminal",
      "envFile": ".env",
      "pythonArgs": [
        "-Xdev"
      ],
      "module": "capellambse.repl",
      "args": [
        "test-5.0"
      ]
    },
    {
      "name": "Interact with the 6.0 test model",
      "type": "python",
      "request": "launch",
      "console": "integratedTerminal",
      "envFile": ".env",
      "pythonArgs": [
        "-Xdev"
      ],
      "module": "capellambse.repl",
      "args": [
        "test-6.0"
      ]
    },
    {
      "name": "Interact with the coffee-maker demo model",
      "type": "python",
      "request": "launch",
      "console": "integratedTerminal",
      "envFile": ".env",
      "pythonArgs": [
        "-Xdev"
      ],
      "module": "capellambse.repl",
      "args": [
        "coffee-maker"
      ]
    }
  ]
}
